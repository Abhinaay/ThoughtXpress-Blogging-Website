import { OnInit, OnDestroy } from '@angular/core';
import { DynamicLoaderService } from './dynamic-loader.service';
export declare class DynamicLoaderComponent implements OnInit, OnDestroy {
    private loaderService;
    /**
     * To set Loader Type
     */
    loaderType: string;
    /**
     * To toggle full screen mode
     */
    isFullScreen: Boolean;
    /**
     * To set top of loader
     */
    top: string;
    /**
     * To set left of loader
     */
    left: string;
    /**
   * To set right of loader
   */
    right: string;
    /**
   * To set bottom of loader
   */
    bottom: string;
    /**
     * Set constant
     */
    constant: {
        LoaderType: {
            rectangleFlip: string;
            circularEffect: string;
        };
    };
    /**
     * To show or hide loader
     */
    isShow: boolean;
    constructor(loaderService: DynamicLoaderService);
    /**
     * Initialization Method
     */
    ngOnInit(): void;
    ngOnDestroy(): void;
}
